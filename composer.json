{
    "name": "ahadcove/cashier-braintree",
    "description": "Laravel Cashier provides an expressive, fluent interface to Braintree's subscription billing services.",
    "keywords": [
        "laravel",
        "braintree",
        "billing",
        "cashier"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ahad Cove",
            "email": "ahadcove@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^7.0|^9.0|^10.0|^11.0|^12.0",
        "braintree/braintree_php": "^6.23.0",
        "dompdf/dompdf": "^v2.0.8",
        "illuminate/contracts": "v11.41.3",
        "illuminate/database": "v11.41.3",
        "illuminate/http": "v11.41.3",
        "illuminate/routing": "v11.41.3",
        "illuminate/support": "v11.41.3",
        "illuminate/view": "v11.41.3",
        "nesbot/carbon": "^3.8.4",
        "symfony/http-kernel": "~2.7|~3.0|~4.0|~5.0|~6.0|~7.0|~8.0"
    },
    "require-dev": {
        "ext-json": "*",
        "mockery/mockery": "~1.6.12",
        "phpunit/phpunit": "~11.5.5"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
